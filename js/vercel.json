{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "framework": null,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "cleanUrls": true,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/api/index.js",
            "status": 200
        }
    ]
}